# Copy to .env.local for local dev. On Railway, set these in the dashboard (Variables).
# Never commit .env or .env.local.

# Gemini API (server-side only). Get key: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

# Firebase Realtime Database. From Firebase Console > Project Settings > Your apps
NEXT_PUBLIC_FIREBASE_DATABASE_URL=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
